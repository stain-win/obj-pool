{
  "name": "obj-pool-obj-pool",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/obj-pool/obj-pool/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/obj-pool/obj-pool",
        "main": "packages/obj-pool/obj-pool/src/index.ts",
        "tsConfig": "packages/obj-pool/obj-pool/tsconfig.lib.json",
        "assets": ["packages/obj-pool/obj-pool/*.md"],
        "generatePackageJson": true
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs obj-pool-obj-pool {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/obj-pool/obj-pool/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/packages/obj-pool/obj-pool"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/packages/obj-pool/obj-pool"
      }
    }
  },
  "tags": []
}
